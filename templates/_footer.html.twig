{% block footer %}
    <!-- Footer -->
    <footer class="footer text-center">
        <div class="container">
            <ul class="list-inline mb-5">
                <li class="list-inline-item">
                    <a class="social-link rounded-circle text-white mr-3" href="https://twitter.com/kara_redwan" title="Lien vers la page Twitter de {{ 'author'|trans }}">
                        <i class="icon-social-twitter"></i>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a class="social-link rounded-circle text-white mr-3" href="https://www.instagram.com/kararedwan/?hl=fr" title="Lien vers la page Instagram de {{ 'author'|trans }}">
                        <i class="icon-social-instagram"></i>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a class="social-link rounded-circle text-white mr-3" href="https://www.linkedin.com/in/redwan-kara-8014a2183/" title="Lien vers la page LinkedIn de {{ 'author'|trans }}">
                        <i class="icon-social-linkedin"></i>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a class="social-link rounded-circle text-white" href="https://github.com/RedwanK" title="Lien vers la page Github de {{ 'author'|trans }}">
                        <i class="icon-social-github"></i>
                    </a>
                </li>
            </ul>
            <p class="text-muted small mb-0">{{ 'app.global.copyright'|trans }} - {{ 'app.global.siret'|trans }}</p>

        </div>
    </footer>

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded js-scroll-trigger" href="#page-top" title="Retour en haut du site">
        <i class="fa fa-angle-up"></i>
    </a>
{% endblock %}

{% block javascript %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
